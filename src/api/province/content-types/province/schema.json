{
  "kind": "collectionType",
  "collectionName": "provinces",
  "info": {
    "singularName": "province",
    "pluralName": "provinces",
    "displayName": "Province"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "provincecode": {
      "type": "string",
      "maxLength": 5,
      "required": true,
      "unique": true
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "provincename": {
      "type": "string",
      "maxLength": 30,
      "required": true
    },
    "recordstatus": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  }
}

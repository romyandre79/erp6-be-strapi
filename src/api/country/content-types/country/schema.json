{
  "kind": "collectionType",
  "collectionName": "countries",
  "info": {
    "singularName": "country",
    "pluralName": "countries",
    "displayName": "Country"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "countrycode": {
      "type": "string",
      "maxLength": 5,
      "required": true,
      "unique": true
    },
    "countryname": {
      "type": "string",
      "default": "30",
      "maxLength": 30,
      "required": true
    },
    "recordstatus": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  }
}
